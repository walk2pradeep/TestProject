<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>GameApp</display-name>
  <welcome-file-list>
    <welcome-file>HomePage.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>StartGameServlet</display-name>
    <servlet-name>StartGameServlet</servlet-name>
    <servlet-class>com.uttara.game.StartGameServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>StartGameServlet</servlet-name>
    <url-pattern>/start</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>GuessServlet</display-name>
    <servlet-name>GuessServlet</servlet-name>
    <servlet-class>com.uttara.game.GuessServlet</servlet-class>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>GuessServlet</servlet-name>
    <url-pattern>/guess</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LeaderBoardServlet</display-name>
    <servlet-name>LeaderBoardServlet</servlet-name>
    <servlet-class>com.uttara.game.LeaderBoardServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LeaderBoardServlet</servlet-name>
    <url-pattern>/leaderBoard</url-pattern>
  </servlet-mapping>
</web-app>